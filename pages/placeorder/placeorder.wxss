/* pages/placeorder/placeorder.wxss */
.wraper{
  width: 100%;
  height: 100vh;
  overflow: hidden;
  box-sizing:border-box;
  position: relative;
}
.item2{
  -webkit-box-flex:2;
  -webkit-flex:2;
  flex:2;
  text-align: center;
  padding: 0px;
}
.item3{
  -webkit-box-flex:3;
  -webkit-flex:3;
  flex:3;
  text-align: center;
  padding: 0px;
}
.item4{
  -webkit-box-flex:4;
  -webkit-flex:4;
  flex:4;
  text-align: center;
  padding: 0px;
}
.item5{
  -webkit-box-flex:5;
  -webkit-flex:5;
  flex:5;
  text-align: center;
  padding: 0px;
}
.item6{
  -webkit-box-flex:6;
  -webkit-flex:6;
  flex:6;
  text-align: center;
  padding: 0px;
}
.itemhf{
  -webkit-box-flex:.5;
  -webkit-flex:.5;
  flex:.5;
  text-align: center;
  padding: 0px;
}
/*bottomwraperstart*/
.wraper .bottomwraper{
  width: 100%;
  height: auto;
  background-color: white;
}
/*bottomwraper信息列表*/
.wraper .bottomwraper .detailposinfo{
  width: 100%;
  overflow: hidden;
  border-top-left-radius: 15rpx;
  border-top-right-radius: 15rpx;
}
.wraper .bottomwraper .detailposinfowrap{
  width: 100%;
  overflow: hidden;
  background-color: white;
  box-sizing: border-box;
  padding: 0px 15rpx;
  padding-top: 20rpx;
}
.wraper .bottomwraper .goodtype{
  width: 100%;
  overflow: hidden;
}
.wraper .bottomwraper .typeitem{
  height: 120rpx;
  overflow: hidden;
}
.wraper .bottomwraper .itemwraper{
  width: 60rpx;
  height: 120rpx;
  margin: 0 auto;
}
.wraper .bottomwraper .itemwraper image{
  width: 60rpx;
  height: 60rpx;
}
.wraper .bottomwraper .itemwraper view{
  width: 60rpx;
  height: 60rpx;
  line-height: 40rpx;
  text-align: center;
  color: #333;
  font-size: 26rpx;
  overflow: hidden;
}
.wraper .bottomwraper .itemwraper view.colored{
  color: #FE8702;
}
.wraper .bottomwraper .needmessage{
  width: 100%;
  height: 130rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #eee;
}
.wraper .bottomwraper .needmessage textarea{
  width: 100%;
  height: 100%;
  font-size: 24rpx;
  color: #333;
  box-sizing: border-box;
  font-weight: lighter;
  padding-left: 20rpx;
  padding-right: 20rpx;
  padding-top: 10rpx;
}
.wraper .bottomwraper .detailpositem{
  height: 100rpx;
  width: 100%;
}
.wraper .bottomwraper .detailpositem .active{
  color: #333;
}
.wraper .bottomwraper .detailpositem.borderbot{
  border-bottom: 1px solid #eee;
}
.wraper .bottomwraper .itemtitle{
  font-size: 30rpx;
  color: #333;
  line-height: 100rpx;
  text-align: left;
  text-indent: .3rem;
}
.wraper .bottomwraper .itemcontent{
  text-align: right;
  line-height: 100rpx;
  color: #999;
  font-size: 28rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.wraper .bottomwraper .addresstip{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  color: #999;
  font-size: 30rpx;
  text-align: right;
}
.wraper .bottomwraper .addressdetail{
  width: 100%;
  height: 100rpx;
  position: relative;
}
.wraper .bottomwraper .addressdetail .ifcall{
  position: absolute;
  bottom: 10rpx;
  left: 10rpx;
  width: 40rpx;
  height: 40rpx;
}
.wraper .bottomwraper .detailinfo{
  width: 100%;
  height: 50rpx;
  color: #666;
  font-size: 26rpx;
  text-align: right;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.wraper .bottomwraper .detailinfo.infotop{
  line-height: 60rpx;
}
.wraper .bottomwraper .detailinfo.infobot{
  line-height: 40rpx;
}
.wraper .bottomwraper .itemcontent input{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: right;
  color: #999;
}
.wraper .bottomwraper .detailposinfowrap .placeholder{
  width: 100%;
  height: 100rpx;
  background-color: white;
}
/*bottomwraper信息列表end*/

/*总计start*/
.subtotal{
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0px;
  left: 0px;
  z-index: 99;
  background-color: white;
  box-shadow: 0px 0px 5px 1px #eee;
}
.subtotal .moneysub{
  font-size: 30rpx;
  line-height: 100rpx;
  text-indent: .5rem;
}
.subtotal .moneysub text{
  color: red;
  font-size: 36rpx;
}
.subtotal .pricedetail{
  color: #333;
  font-size: 30rpx;
  line-height: 100rpx;
}
