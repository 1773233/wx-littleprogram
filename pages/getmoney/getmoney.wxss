/* pages/getmoney/getmoney.wxss */
page{
  background-color: #f5f5f5;
}
.wraper{
  width: 100%;
  height: auto;
  padding-bottom: 100rpx;
  background-color: #f5f5f5;
}
.item2{
  -webkit-box-flex:2;
  -webkit-flex:2;
  flex:2;
  text-align: center;
  padding: 0px;
}
.item3{
  -webkit-box-flex:3;
  -webkit-flex:3;
  flex:3;
  text-align: center;
  padding: 0px;
}
.item4{
  -webkit-box-flex:4;
  -webkit-flex:4;
  flex:4;
  text-align: center;
  padding: 0px;
}
.item5{
  -webkit-box-flex:5;
  -webkit-flex:5;
  flex:5;
  text-align: center;
  padding: 0px;
}
.item6{
  -webkit-box-flex:6;
  -webkit-flex:6;
  flex:6;
  text-align: center;
  padding: 0px;
}
.arrowright{
  width: 12rpx;
  height:26rpx;
  margin-top: 70rpx;
  float: right;
  margin-right: 50rpx;
}
.arrowright image{
  width: 100%;
  height: 100%;
}
.nav{
  width: 100%;
  height: 80rpx;
  background-color: #131426;
  text-align: right;
  margin-bottom: 20rpx;
  box-shadow: 0px 1rpx 5rpx 1rpx #eee;
}
.nav .jl{
  color: #fff;
  font-size: 30rpx;
  letter-spacing: 2rpx;
  line-height: 80rpx;
  padding-right: 50rpx;
}
.withmargin{
  margin-top: 30rpx;
}
.block{
  height: 100rpx;
  width: 100%;
  background-color: #fff;
  border-bottom: 1px solid #eee;
}
.block .itemleft{
  color: #333;
  font-size: 32rpx;
  line-height: 100rpx;
  text-align: left;
  margin-left: 20rpx;
  float: left;
}
.block .itemleft text{
  color: #fd8701;
  font-size: 28rpx;
}
.block .itemleft .bigwrods{
  font-size: 50rpx;
  color: #000;
  float: left;
}
.block .itemleft input{
  float: left;
  height: 100rpx;
  width: 280rpx;
  font-size: 30rpx;
  line-height: 100rpx;
  text-align: left;
  margin-left: 20rpx;
  color: #000;
  font-weight: bold;
}
.holderstyle{
  color: #999;
  font-size: 28rpx;
  line-height: 100rpx;
  text-align: left;
  text-indent: 20rpx;
  font-weight: lighter;
}
.block .itemright{
  color: #fd8701;
  font-size: 32rpx;
  line-height: 100rpx;
  text-align: right;
  margin-right: 20rpx;
  float: right;
}
.block .itemright .smallwords{
  color: #999;
  font-size: 28rpx;
  line-height: 100rpx;
}
.cardlist{
  width: 100%;
  height: auto;
}
.cardlist .blockbig{
  width: 100%;
  height: 200rpx;
  background-color: #fff;
  border-bottom: 1px solid #eee;
}
.cardlist .icon{
  width: 100%;
  height: 200rpx;
  text-align: left;
}
.cardlist .icon image{
  width: 80rpx;
  height: 80rpx;
  margin-top: 60rpx;
  margin-left: 20rpx;
}
.cardlist .bankname{
  height: 100rpx;
  line-height: 140rpx;
  color: #000;
  font-size: 32rpx;
  text-align: left;
}
.cardlist .carddesc{
  height: 100rpx;
  line-height: 60rpx;
  color: #666;
  font-size: 30rpx;
  text-align: left;
}
.cardlist .tipname{
  font-size: 32rpx;
  color: #000;
  line-height: 200rpx;
  width: 100%;
  text-align: left;
}
.cardlist .select{
  width: 50rpx;
  height:46rpx;
  margin-top: 70rpx;
  float: right;
  margin-right: 30rpx;
}
.cardlist .select image{
  width: 100%;
  height: 100%;
}
.tips{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: left;
  font-size: 28rpx;
  color: #666;
  text-indent: 20rpx;
  background-color: #fff;
  border-top: 1px solid #eee;
}
.btn{
  width: 80%;
  margin-left: 10%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
  margin-top:60rpx;
  background-color: #fd8701;
}
.noborder{
  border: none;
}