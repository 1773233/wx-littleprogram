/* pages/aboutus/aboutus.wxss */
.wraper{
  width: 100%;
  height: 100vh;
  box-sizing: border-box;
  border: 1rpx solid #fff;
}
.item2{
  -webkit-box-flex:2;
  -webkit-flex:2;
  flex:2;
  text-align: center;
  padding: 0px;
}
.item3{
  -webkit-box-flex:3;
  -webkit-flex:3;
  flex:3;
  text-align: center;
  padding: 0px;
}
.item4{
  -webkit-box-flex:4;
  -webkit-flex:4;
  flex:4;
  text-align: center;
  padding: 0px;
}
.item5{
  -webkit-box-flex:5;
  -webkit-flex:5;
  flex:5;
  text-align: center;
  padding: 0px;
}
.item6{
  -webkit-box-flex:6;
  -webkit-flex:6;
  flex:6;
  text-align: center;
  padding: 0px;
}
.logo{
  width: 100%;
  height: 150rpx;
  text-align: center;
  margin-top:150rpx; 
}
.logo image{
  width: 120rpx;
  height: 120rpx;
}
.title{
  font-size: 30rpx;
  color: #333;
  line-height: 80rpx;
  width: 100%;
  text-align: center;
}
.version{
  font-size: 26rpx;
  line-height: 80rpx;
  color: #555;
  width: 100%;
  text-align: center;
}
.little{
  font-size: 28rpx;
  color: #999;
  line-height: 50rpx;
  width: 100%;
  text-align: center;
}
.telcall{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 30rpx;
  color: #333;
  position: absolute;
  bottom: 20rpx;
}
.telcall text{
  margin-left: 10rpx;
  color: #fe8b03;
}